<h1>Photos</h1>

<% for photo in @photos -%>
  <br />
  <tr>
    <td><%= image_tag photo.public_filename (:thumb) %></td>
    <td><%= link_to 'Show', :action => 'show', :id => photo %></td> |
    <td><%= link_to "Destroy", photo, :confirm => "Are you sure?", :method => :delete %></td>
  </tr>
<% end -%>