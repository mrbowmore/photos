<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', votes_path  %></li>
      <li><%= link_to 'New', new_vote_path %></li>
      <li class="active"><%= link_to 'Show', vote_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>user_id:</b>
